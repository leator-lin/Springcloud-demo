<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.define.dao.UserDao">

    <select id="getUsers" resultType="User">
        SELECT
            u.id,
            u.username,
            u.password,
            u.address,
            u.email
        FROM
          user u
    </select>

</mapper>